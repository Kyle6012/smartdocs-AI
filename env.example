# Port
PORT=3000

# Together AI API (for chat and embeddings)
TOGETHER_API_KEY=your_together_api_key_here
TOGETHER_MODEL=meta-llama/Llama-2-70b-chat-hf

# Qdrant Configuration (collection will be auto-created if not exists)
QDRANT_URL=https://your-cluster-url.aws.cloud.qdrant.io:6333
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_COLLECTION_NAME=knowledge_base

# WhatsApp Configuration (Choose ONE option)

# Option 1: Baileys (Free, requires QR scan)
WA_SESSION_NAME=wa-bot-session
USE_BAILEYS=true

# Option 2: WhatsApp Business API (Paid, no QR scan)
USE_WHATSAPP_BUSINESS=false
WA_BUSINESS_ACCESS_TOKEN=your_access_token_here
WA_BUSINESS_PHONE_NUMBER_ID=your_phone_number_id_here
WA_BUSINESS_WEBHOOK_VERIFY_TOKEN=your_webhook_verify_token_here
WA_BUSINESS_API_VERSION=v18.0

# Admin Numbers (comma separated, with country codes)
ADMIN_NUMBERS=+1234567890,+9876543210

# Optional: Logging level
LOG_LEVEL=info 