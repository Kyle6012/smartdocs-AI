{
  "name": "wa-agent",
  "version": "1.0.0",
  "description": "AI WhatsApp Bot with Baileys, Together AI, and Qdrant vector database",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prestart": "npm run build",
    "ingest": "tsx scripts/ingest-knowledge.ts",
    "ingest:build": "tsc && node dist/scripts/ingest-knowledge.js",
    "test:widget": "echo 'Open test-widget.html in your browser after running npm run dev'"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "ai",
    "baileys",
    "together-ai",
    "qdrant",
    "llama",
    "vector-database"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@qdrant/js-client-rest": "^1.8.2",
    "@whiskeysockets/baileys": "^6.7.18",
    "axios": "^1.6.5",
    "chalk": "^4.1.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mammoth": "^1.6.0",
    "pdf-parse": "^1.1.1",
    "qrcode-terminal": "^0.12.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pdf-parse": "^1.1.4",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
